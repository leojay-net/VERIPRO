# VeriPro Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Google Gemini API Key (required for AI spec generation)
GOOGLE_API_KEY=your_gemini_api_key

# WalletConnect Project ID (required for wallet connections)
# Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id

# Attestation Registry Contract Address (Mantle Sepolia)
# Default: 0xae454F272197b110C28223dbE3e49b4a1c798015
NEXT_PUBLIC_ATTESTATION_REGISTRY_ADDRESS=0xae454F272197b110C28223dbE3e49b4a1c798015
